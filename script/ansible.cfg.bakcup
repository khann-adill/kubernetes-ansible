[defaults]
roles_path = ./roles
inventory  = /etc/ansible/custom_inv.ini
remote_user = ubuntu
remote_tmp = $HOME/.ansible/tmp
local_tmp  = $HOME/.ansible/tmp
pipelining = True
become = True
host_key_checking = False
deprecation_warnings = False
callback_whitelist = profile_tasks
private_key_file = /root/kubernetes-ansible/id_rsa_aws
